{
    "name": "laravolt/comma",
    "description": "Content management for Laravel",
    "keywords": [
        "laravel",
        "laravolt",
        "blog",
        "CMS"
    ],
    "homepage": "https://github.com/laravolt/comma",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/database": "5.6.*",
        "illuminate/support": "5.6.*",
        "spatie/laravel-sluggable": "^2.1.3",
        "spatie/laravel-medialibrary": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "orchestra/testbench": "~3.2.0|~3.3.0|~3.4.0@dev",
        "orchestra/database": "~3.2.0|~3.3.0|~3.4.0@dev",
        "mockery/mockery": "~0.9.4"
    },
    "suggest": {
        "laravel/scout": "Required to utilize Searchable Post"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Comma\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Comma\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Comma\\ServiceProvider"
            ]
        }
    }
}
