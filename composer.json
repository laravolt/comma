    "keywords": [
        "laravel",
        "laravolt",
        "blog",
        "CMS"
    ],
    "homepage": "https://github.com/laravolt/comma",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0.0",
        "illuminate/database": "~5.3.0|~5.4.0",
        "illuminate/support": "~5.3.0|~5.4.0",
        "spatie/laravel-sluggable": "^1.4"
    },
    "require-dev": {
